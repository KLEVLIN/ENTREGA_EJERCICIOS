Proceso PROMEDIO_CALIFICACIONES
	Definir CANTIDAD_ESTUDIANTES, CONTADOR Como Entero;
	DEFINIR NOTA, SUMA, PROMEDIO_NOTASGRUPAL Como Real;
	Escribir "INGRESE LA CANTIDAD DE ESTUDIANTES EN EL CURSO: ";
	Leer CANTIDAD_ESTUDIANTES;
	CONTADOR<-0;
	Para CONTADOR<- 1 Hasta CANTIDAD_ESTUDIANTES Con Paso 1 Hacer;
		Escribir "INGRESE LA CALIFICACION: ";
		Leer NOTA;
		SUMA<-SUMA + NOTA;
	FinPara
	PROMEDIO_NOTAGRUPAL<- SUMA/CANTIDAD_ESTUDIANTES;
	Escribir "EL PROMEDIO GENERAL DEL CURSO ES: ", PROMEDIO_NOTAGRUPAL;
FinProceso